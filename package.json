{
  "name": "stock-prompt-extension",
  "version": "1.3.0",
  "description": "A modern prompt storage browser extension with stock query and history",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "tsc && vite build && node scripts/post-build.js",
    "preview": "vite preview",
    "setup-icons": "powershell -ExecutionPolicy Bypass -File scripts/setup-icons.ps1"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.344.0",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/chrome": "^0.0.268",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

